{
    "manifest_version": 3,
    "name": "Change_background",
    "description": "Add a custom background to google.com",
    "version": "1.0",
    "icons": {
        "16": "icon.png"
    },
    "page_action": {
    "default_icon" : "icon.png"
  },

  "content_scripts": [{
    "matches": ["https://www.google.com/*"],
 
  "css": ["style.css"]
  }]
}